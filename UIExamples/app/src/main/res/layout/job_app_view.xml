<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".JobAppView"
        android:background="@color/cardview_dark_background">

    <TextView
            android:id="@+id/text_title"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.592"
            app:layout_constraintVertical_bias="0.044"
            android:text="Job Application"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:id="@+id/text_Nombre"
            android:layout_marginRight="1dp"
            android:layout_marginLeft="10dp"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            android:textSize="20sp"
            android:textColor="#C2C2C2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.828"
            app:layout_constraintVertical_bias="0.169"
            android:text="Nombre" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:layout_marginRight="1dp"
            android:layout_marginLeft="10dp"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            android:textSize="20sp"
            android:id="@+id/txt_startDate"
            android:textColor="#C2C2C2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.836"
            app:layout_constraintVertical_bias="0.267"
            android:text="usuario" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Id"
            android:layout_marginRight="1dp"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            android:textSize="20sp"
            android:textColor="#C2C2C2"
            android:id="@+id/txt_id"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.84"
            app:layout_constraintVertical_bias="0.38" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Address"
            android:layout_marginRight="1dp"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:textColor="#C2C2C2"
            android:textAlignment="center"
            android:layout_marginLeft="10dp"
            android:textSize="20sp"
            android:id="@+id/addres_txt"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.492" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.835"/>
    <TextView
            android:text="email"
            android:layout_marginRight="1dp"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:textColor="#C2C2C2"
            android:textAlignment="center"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:id="@+id/email_txt"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.593"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.806" app:layout_constraintStart_toStartOf="parent"/>

    <TextView
            android:text="Phone"
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            android:textColor="#C2C2C2"
            android:textSize="20sp"
            android:id="@+id/phone_txt"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.809"
            app:layout_constraintVertical_bias="0.68" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:layout_width="259dp"
            android:layout_height="38dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            android:textSize="20sp"
            android:textColor="#C2C2C2"
            android:id="@+id/position_txt"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.794"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="Position"
            android:layout_marginRight="1dp"
            app:layout_constraintHorizontal_bias="0.815" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Name:"
            android:layout_marginLeft="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/name"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.082" app:layout_constraintVertical_bias="0.178"
            android:capitalize="characters" android:textStyle="bold" android:textSize="20sp"/>
    <TextView
            android:text="Start Date:"
            android:layout_marginLeft="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView17"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.079"
            app:layout_constraintVertical_bias="0.273" android:textSize="20sp" android:textStyle="bold"/>
    <TextView
            android:text="Id:"
            android:layout_marginLeft="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView18"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintVertical_bias="0.381" android:textStyle="bold" android:textSize="20sp"/>
    <TextView
            android:text="Address:"
            android:layout_marginLeft="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView19"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.076"
            app:layout_constraintVertical_bias="0.484" android:textStyle="bold" android:textSize="20sp"/>
    <TextView
            android:text="E-Mail:"
            android:layout_marginLeft="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView20"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.076" app:layout_constraintVertical_bias="0.59"
            android:textStyle="bold" android:textSize="20sp"/>
    <TextView
            android:text="Phone:"
            android:layout_marginLeft="1dp"
            android:layout_width="80dp"
            android:layout_height="21dp" android:id="@+id/textView21"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.084"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintVertical_bias="0.691"
            android:textStyle="bold" android:textSize="20sp"/>
    <TextView
            android:text="Position:"
            android:layout_marginLeft="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView22"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.078"
            app:layout_constraintVertical_bias="0.802" android:textStyle="bold" android:textSize="20sp"/>
    <com.google.android.material.button.MaterialButton
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnBack"
            android:backgroundTint="@color/black"
            android:layout_centerHorizontal="true"
            android:text="BACK"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintVertical_bias="0.936"/>
</androidx.constraintlayout.widget.ConstraintLayout>